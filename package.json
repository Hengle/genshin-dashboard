{
  "name": "genshin-dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "dev-inspect": "cross-env NODE_ENV=development NODE_OPTIONS=--inspect next",
    "build": "next build",
    "start": "next start",
    "compile:test": "tsc",
    "lint:fix": "eslint src --fix",
    "lint:test": "eslint src",
    "prettier:fix": "prettier \"src/**/*.ts(x)?\" --write",
    "prettier:test": "prettier \"src/**/*.ts(x)?\" --list-different",
    "prepare": "husky install",
    "precommit": "npm run compile:test && npm run lint:fix && npm run prettier:fix"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.2.3",
    "antd": "^4.16.2",
    "autoprefixer": "^10.2.5",
    "crypto": "^1.0.1",
    "lodash": "^4.17.21",
    "next": "^10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "next-seo": "^4.24.0",
    "node-fetch": "^2.6.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.0.2",
    "@types/node-fetch": "^2.5.10",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-highlight-words": "^0.16.2",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
